distNormalSimulacao <- rnorm(70)
print(distNormalSimulacao)
summary(distNormalSimulacao)
distBinominalSimulacao <- rbinom(120, 1, 0.3)
print(distBinominalSimulacao)
summary(distBinominalSimulacao)
membresiaUE <- c(rep("país_membro_da_UE", length(distBinominalSimulacao)/2), rep("país_não_é_da_UE", length(distBinominalSimulacao)/2)) 
print(membresiaUE)
indexSimulacao <- seq(1, length(distNormalSimulacao))
print(indexSimulacao)
